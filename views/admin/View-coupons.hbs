                <h5 class="mb-0">Applications</h5>
            </div>
            <div class="table-responsive">
                <table class="table table-hover table-nowrap">
                    <thead class="thead-light">
                        <tr>

                            <th scope="col">#</th>
                            <th scope="col">Coupon Name</th>
                            <th scope="col">Coupon Code</th>
                            <th scope="col">Discount Rs</th>
                            <th scope="col">Limit</th>
                            <th scope="col">Created On</th>
                            <th scope="col">Expire On</th>
                            <th scope="col"></th>


                        </tr>
                    </thead>
                    <tbody>

                        {{#each copounsDetails}}
                        <tr>

                            <td>1</td>
                            <td> {{this.couponName}}</td>
                            <td>{{this.couponCode}}</td>
                            <td>{{this.discount}}</td>
                            <td>{{this.limit}}</td>
                            <td>{{thiss.createdAt}}</td>
                            <td>{{thiss.expirationTime}}</td>


                            <td class="text-end">
                                <a href="/admin/room-view/{{this._id}}"
                                    class="btn btn-sm btn-square btn-neutral text-danger-hover me-1">
                                    <i class="fa-solid fa-users"></i> </a>
                                <a href="/admin/edit-coupon?id={{this._id}}"
                                    class="btn btn-sm btn-square btn-neutral text-danger-hover"> <i class="bi bi-pen">
                                    </i></a>
                                <a href="/admin/delete-coupon/{{this._id}}"
                                    class="btn btn-sm btn-square btn-neutral text-danger-hover"
                                    onclick="return confirm('Are you sure you want to Remove {{this.couponName}}?');">
                                    <i class="bi bi-trash"></i> </a>
                            </td>


                        </tr>
                        {{/each}}

                    </tbody>
                </table>
            </div>
            <div class="card-footer border-0 py-5"> <span class="text-muted text-sm">Showing 10 items out of 250
                    results found</span> </div>
        </div>
    </div>
