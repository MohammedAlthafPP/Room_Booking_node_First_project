<!-- Banner -->
<!-- Main      viewBookingTotalCount,RoomsTotalCount,UsersTotalCount-->

<main class="py-6 bg-surface-secondary">
    <div class="container-fluid">
        <!-- Card stats -->
        <div class="row g-6 mb-6">
            <div class="col-xl-3 col-sm-6 col-12">
                <div class="card shadow border-0">
                    <div class="card-body">
                        <div class="row">
                            <div class="col"> <span class="h6 font-semibold text-muted text-sm d-block mb-2">Total
                                    Rooms</span> <span class="h3 font-bold mb-0">{{RoomsTotalCount}}</span> </div>
                            <div class="col-auto">
                                <div class="icon icon-shape bg-info text-white text-lg rounded-circle"> <i
                                        class="bi-building"></i> </div>
                            </div>
                        </div>
                        <div class="mt-2 mb-0 text-sm"> <span
                                class="badge badge-pill bg-soft-success text-success me-2"> <i
                                    class="bi bi-arrow-up me-1"></i>13% </span> <span
                                class="text-nowrap text-xs text-muted">Since last month</span> </div>
                    </div>
                </div>
            </div>
            <div class="col-xl-3 col-sm-6 col-12">
                <div class="card shadow border-0">
                    <div class="card-body">
                        <div class="row">
                            <div class="col"> <span class="h6 font-semibold text-muted text-sm d-block mb-2">Total
                                    Users</span> <span class="h3 font-bold mb-0">{{UsersTotalCount}}</span> </div>
                            <div class="col-auto">
                                <div class="icon icon-shape bg-primary text-white text-lg rounded-circle"> <i
                                        class="bi bi-people"></i> </div>
                            </div>
                        </div>
                        <div class="mt-2 mb-0 text-sm"> <span
                                class="badge badge-pill bg-soft-success text-success me-2"> <i
                                    class="bi bi-arrow-up me-1"></i>30% </span> <span
                                class="text-nowrap text-xs text-muted">Since last month</span> </div>
                    </div>
                </div>
            </div>

            <div class="col-xl-3 col-sm-6 col-12">
                <div class="card shadow border-0">
                    <div class="card-body">
                        <div class="row">
                            <div class="col"> <span class="h6 font-semibold text-muted text-sm d-block mb-2">Total
                                    Bookings</span>
                                <span class="h3 font-bold mb-0">{{viewBookingTotalCount}}</span>
                            </div>
                            <div class="col-auto">
                                <div class="icon icon-shape bg-tertiary text-white text-lg rounded-circle"> <i
                                        class="bi bi-credit-card"></i> </div>
                            </div>
                        </div>
                        <div class="mt-2 mb-0 text-sm"> <span class="badge badge-pill bg-soft-danger text-danger me-2">
                                <i class="bi bi-arrow-down me-1"></i>-5% </span> <span
                                class="text-nowrap text-xs text-muted">Since last month</span> </div>
                    </div>
                </div>
            </div>

            <div class="col-xl-3 col-sm-6 col-12">
                <div class="card shadow border-0">
                    <div class="card-body">
                        <div class="row">
                            <div class="col"> <span class="h6 font-semibold text-muted text-sm d-block mb-2">Total
                                    Earnings</span>
                                <span class="h3 font-bold mb-0"><span><i class="fa fa-inr"></i>
                                    </span>{{TotalEarned}}/-</span>
                            </div>
                            <div class="col-auto">
                                <div class="icon icon-shape bg-success text-white text-lg rounded-circle">
                                    {{!-- <i class="bi-currency-exchange"></i> --}}
                                    <i class="fa fa-inr"></i>
                                    {{!-- <i class="bi-currency-exchange"></i> --}}
                                </div>

                            </div>
                        </div>
                        <div class="mt-2 mb-0 text-sm"> <span
                                class="badge badge-pill bg-soft-success text-success me-2"> <i
                                    class="bi bi-arrow-up me-1"></i>10% </span> <span
                                class="text-nowrap text-xs text-muted">Since last month</span> </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="card shadow border-0 mb-7">
            <div class="card-header">
                <h5 class="mb-0">Applications</h5>
            </div>
            <div class="table-responsive">
                <table class="table table-hover table-nowrap">
                    <thead class="thead-light">
                        <tr>
                            <th scope="col">User</th>
                            <th scope="col">Room</th>
                            <th scope="col">Check-in</th>
                            <th scope="col">Checkout</th>
                            <th scope="col">Payment</th>
                            <th scope="col">Status</th>
                            <th></th>
                        </tr>
                    </thead>
                    <tbody>
                        {{!-- <p>BookingDetailsComp[0].User_id</p>
                        <p>{{BookingDetailsComp.[0].User_id.[0].name}}</p>
                        <p>{{BookingDetailsComp.[0].User_id}}</p> --}}
                        {{#each BookingDetailsComp}}
                        <tr class="mb-5">
                            {{#each this.User_id}}
                            <td > <img alt="..." src="/images/User-profile/{{this.user_img}}"
                                    class="avatar avatar-sm rounded-circle me-2"> <a class="text-heading font-semibold text-truncate para "
                                    href="#">{{this.name}}</a> </td>
                            {{/each}}

                            <td class="text-truncate para"> <img alt="..." src="/images/rooms-img/{{this.room.all_images.[0].main_img}}"
                                    class="avatar avatar-xs rounded-circle me-2"> <a class="text-heading font-semibold "
                                    href="#" > {{this.room.room_name}} </a> </td>
                            <td> {{this.checkin}}</td>
                            <td> {{this.checkout}} </td>
                            <td>
                                <span class="badge badge-lg badge-dot">{{this.paymentMethod}}
                                </span>
                            </td>
                            <td>
                                {{#if isStatusBooked}}
                                <span class="badge badge-lg badge-dot"> <i class="bg-success"></i>{{this.Status}}
                                </span>
                                {{else}}
                                <span class="badge badge-lg badge-dot"> <i class="bg-danger"></i>{{this.Status}}
                                </span>
                                {{/if}}
                            </td>
                            <td class="text-end"> <a href="/admin/single-booking?id={{this._id}}"
                                    class="btn btn-sm btn-neutral">More</a> <button type="button"
                                    class="btn btn-sm btn-square btn-neutral text-danger-hover">
                                    <i class="bi bi-trash"></i> </button> </td>
                        </tr>
                        {{/each}}








